<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Numbula">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/numbulaicon"
        android:label="Numbula"
        android:roundIcon="@mipmap/numbulaicon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.Numbula.MainMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.Numbula.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.DEFAULT"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>

</manifest>